<!DOCTYPE html>
<html>
<head>
  <title>Dashboard</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <link href="http://localhost:3000/plugins/bootstrap/bootstrap.css" rel="stylesheet">
  <link href="http://localhost:3000/css/style.css" rel="stylesheet">
  <link href="http://netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css" rel="stylesheet">
  <link href='http://fonts.googleapis.com/css?family=Righteous' rel='stylesheet' type='text/css'>
  <link href="http://localhost:3000/plugins/select2/select2.css" rel="stylesheet">
</head>
<body>

<div id="main" class="container-fluid">
	<div class="row"> 
    <%= render :partial => 'shared/navbar' %>
    <%= render :partial => 'shared/sidebar' %>

		<div id="content" class="col-xs-12 col-sm-10">
			<div id="ajax-content">
        <%= yield %>
        <script src="http://code.jquery.com/jquery-1.10.1.min.js"></script>
<script>
  $.ajax({
  url:"https://www.kimonolabs.com/api/4oiqgona?apikey=IqBA7ChK1le70NM5QZLSPKdLTRtX3pIv",
  beforeSend: function(xhr) { xhr.setRequestHeader('authorization', 'Bearer MtqDhEJr3QGCrZL4Oyi4Sm63dLpDLFwt'); },
  success: function (response) {
    //Do something with the response
  },
  error: function (xhr, status) {
    //handle errors
  }
});</script>
      </div>
    </div>
  </div>
</div>

    <% flash.each do |name, msg| %>
      <%= content_tag :div, msg, :id => "flash_#{name}", :class => "alert alert-info alert-dismissible", :role => "alert" do %>
        <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <%= content_tag :strong, msg %>
        <% end %>
    <% end %>

<!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
<!--<script src="http://code.jquery.com/jquery.js"></script>-->
<script src="http://localhost:3000/plugins/jquery/jquery-2.1.0.min.js"></script>
<script src="http://localhost:3000/plugins/jquery-ui/jquery-ui.min.js"></script>
<!-- Include all compiled plugins (below), or include individual files as needed -->
<script src="http://localhost:3000/plugins/bootstrap/bootstrap.min.js"></script>
<script src="http://localhost:3000/plugins/justified-gallery/jquery.justifiedgallery.min.js"></script>
<script src="http://localhost:3000/plugins/tinymce/tinymce.min.js"></script>
<script src="http://localhost:3000/plugins/tinymce/jquery.tinymce.min.js"></script>
<!-- All functions for this theme + document.ready processing -->
<script src="http://localhost:3000/js/devoops.js"></script>

</body>
</html>